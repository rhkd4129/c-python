#include <stdio.h>




int main() {

	int num;
	int number = 1;
	scanf("%d", &num);
	int i, j;
	int array[100][100];


	for ( j = 0; j < num; j++) {
		for ( i = num - 1; i >= 0; i--, number++) {
			array[j][i] = number;

		}
	}

	for (j = 0; j < num; j++) {
		for (i = 0; i < num; i++) {
			printf("%d", array[j][i]);
		}
		printf("\n");
	}

}
